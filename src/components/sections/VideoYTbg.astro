---
const videoURL= "https://www.youtube.com/embed/"
const videoParams = "?controls=0&autoplay=1&mute=1&start=5&end=50&playsinline=1&loop=1&playlist="
const videoID= "3YfspCI8-Ec&t"


---

 
 <div class="video-container opacity-80 z-0 w-full h-screen absolute top-0 left-0 overflow-hidden ">
  <div class="overlay bg-slate-100 w-full h-screen absolute top-0 left-0 z-1 opacity-60 ">
    <img src="" alt="">
  </div>
	<iframe loading="lazy" src={videoURL+videoID+videoParams+videoID} ></iframe>
   
  </div>

<!-- webkitallowfullscreen mozallowfullscreen allowfullscreen autoplay -->

<style>

iframe :before {
  content: "";
  display: block;
  position: absolute;
  width: "100%";
  background: orangered;
  height: "300px;";
  z-index: 999;




}  

 iframe {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 100%;
  height: 100%;
  transform: translate(-50%, -50%);
  pointer-events: none;
}

@media (min-aspect-ratio: 16/9) {
  .video-container iframe {
    height: 56.25vw;
  }
}
@media (max-aspect-ratio: 16/9) {
  .video-container iframe {
    width: 177.78vh;
  }
}
</style>